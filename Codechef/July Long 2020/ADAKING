// Code of https://www.codechef.com/JULY20B/problems/ADAKING
// Note- Various possible solutions are there for this Problem..
//       This is the easiest approach according to me...

#include<bits/stdc++.h>
using namespace std;
int main() {
    ios_base::sync_with_stdio(false); cin.tie(NULL);
    int t;
    cin>>t;
    while(t--){
        int k,i,j,c=1;
        cin>>k;
        for(i=0;i<8;i++){
            for(j=0;j<8;j++){
                if(i==0 && j==0) cout<<"O";
                else if(c<k){
                    cout<<".";
                    c+=1;
                }
                else cout<<"X";
            } cout<<"\n"; }
            //cout<<"\n";
    }
    return 0;
}
